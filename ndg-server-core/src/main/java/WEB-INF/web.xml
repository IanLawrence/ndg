<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app>
	
	<!--  
	Usado no menu SMS
	-->
	<servlet>
		<servlet-name>StartUpListenerServlet</servlet-name>
		<servlet-class>br.org.indt.ndg.server.core.servlet.StartUpListenerServlet</servlet-class>
		<load-on-startup>30</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>StartUpListenerServlet</servlet-name>
		<url-pattern>/startUpListenerServlet</url-pattern>
	</servlet-mapping>

	<error-page>
		<error-code>404</error-code>
		<location>/error404.html</location>
	</error-page>
</web-app>
